<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent" android:clipChildren="false">

	<ImageView android:id="@+id/imageView2"
		android:layout_width="90dp" android:layout_height="90dp"
		android:layout_centerHorizontal="true"
		android:layout_centerVertical="true"
		android:src="@drawable/welcome_screen_feedback" />

	<ImageView android:id="@+id/imageView5"
		android:layout_width="100dp" android:layout_height="100dp"
		android:layout_alignLeft="@+id/imageView3"
		android:layout_alignStart="@+id/imageView3"
		android:layout_below="@+id/imageView2"
		android:src="@drawable/welcome_screen_feedback" />

	<ImageView android:id="@+id/imageView"
		android:layout_width="110dp" android:layout_height="110dp"
		android:layout_alignLeft="@+id/imageView4"
		android:layout_alignStart="@+id/imageView4"
		android:layout_alignTop="@+id/imageView3"
		android:layout_marginTop="38dp"
		android:src="@drawable/welcome_screen_feedback" />

	<ImageView android:id="@+id/imageView3"
		android:layout_width="120dp" android:layout_height="120dp"
		android:layout_above="@+id/imageView2"
		android:layout_toLeftOf="@+id/imageView2"
		android:layout_toStartOf="@+id/imageView2"
		android:src="@drawable/welcome_screen_feedback" />

	<ImageView android:id="@+id/imageView4"
		android:layout_width="130dp" android:layout_height="130dp"
		android:layout_alignTop="@+id/imageView5"
		android:layout_marginTop="36dp"
		android:layout_toEndOf="@+id/imageView2"
		android:layout_toRightOf="@+id/imageView2"
		android:src="@drawable/welcome_screen_feedback" />

</RelativeLayout>