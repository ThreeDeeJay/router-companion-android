<resources>
  <declare-styleable name="TextProgressBar">
    <attr format="string" name="text"/>
    <attr format="color" name="textColor"/>
    <attr format="dimension" name="textSize"/>
  </declare-styleable>
  <declare-styleable name="LineView">
    <attr format="reference" name="from"/>
    <attr format="reference" name="to"/>
  </declare-styleable>
  <declare-styleable name="NumberPickerPreference">
    <attr format="integer" name="max"/>
    <attr format="integer" name="min"/>
    <attr format="string" name="maxExternal"/>
    <attr format="string" name="minExternal"/>
    <attr format="string" name="description"/>
  </declare-styleable>

  <declare-styleable name="DoubleNumberPickerPreference">
    <attr format="integer" name="defaultValue1"/>
    <attr format="integer" name="defaultValue2"/>
    <attr format="integer" name="max1"/>
    <attr format="integer" name="min1"/>
    <attr format="integer" name="max2"/>
    <attr format="integer" name="min2"/>
    <attr format="string" name="maxExternal1"/>
    <attr format="string" name="minExternal1"/>
    <attr format="string" name="maxExternal2"/>
    <attr format="string" name="minExternal2"/>
    <attr format="string" name="pickerTitle1"/>
    <attr format="string" name="pickerTitle2"/>
  </declare-styleable>
</resources>